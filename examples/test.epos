# test.epos

fn call_fn(caller_fn: fn(int) -> int, nums: list(int), index: int, result_list: list(int)): list(int)
  match index < len(nums) then
    true -> call_fn(caller_fn, nums, index + 1, {..result_list, caller_fn(elem(nums, index))})
    _ -> result_list
  end
end
fn double(a: int): int
  a * 2
end
print(call_fn(double, {1, 2, 3, 4, 5}, 0, {0}))
